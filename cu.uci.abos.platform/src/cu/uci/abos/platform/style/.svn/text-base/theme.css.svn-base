Label.customLabel {
	color: #FFFFFF;
	background-color: transparent;
	opacity: .6;
}

Label {
	color: #004682;
	background-color: transparent;
	font: 12px Verdana, "Lucida Sans", Arial, Helvetica, sans-serif;
}

Label.applicationName {
	color: #004682;
	background-color: transparent;
	font: bold 14px Verdana, "Lucida Sans", Arial, Helvetica, sans-serif;
}

Label.Banner {
	background-image: url( "/cu/uci/abos/platform/resources/Banner.png" );
	background-repeat: no-repeat;
}

Label.gradient {
	background-image: url( "/cu/uci/abos/platform/resources/gradient.png" );
	background-repeat: no-repeat;
}

Label.Logo {
	background-image: url( "/cu/uci/abos/platform/resources/Logo.png" );
	background-repeat: no-repeat;
}

Label.Login_grafica {
	background-image: url( "/cu/uci/abos/platform/resources/Fondo.png" );
}

Label.grafica {
	background-image: url( "/cu/uci/abos/platform/resources/grafica.jpg" );
	background-repeat: no-repeat;
}

Label.footer_separator {
	background-image:
		url( "/cu/uci/abos/platform/resources/footer_separator.png" );
}

Label.upload-selection {
	border-radius: 2px;
}

/*Estilo de los botones*/
Button[CHECK] {
	vertical-align: middle;
	spacing: 7px;
	color: #004682;
	background-color: transparent;
	font: 12px Verdana, "Lucida Sans", Arial, Helvetica, sans-serif;
	text-align: center;
}

Button.menu_button {
	color: #ffffff;
	background-color: transparent;
	background-image: none;
	padding: 3px 6px 3px 4px;
	font: bold 18px Arial, Helvetica, sans-serif;
	border: none;
}

Button.menu_button:hover {
	border: 1px solid #A4A4A4;
}

Button[PUSH]:hover {
	color: #ffffff;
	background-color: #0044cc;
	background-image: gradient(linear, left top, left bottom, from(#226DA5),
		to(#226DA5));
	background-repeat: repeat-x;
	text-shadow: 0px -px 0px rgba(255, 255, 255, 0.4);
	font: 14px Verdana, "Lucida Sans", Arial, Helvetica, sans-serif;
}

Button[PUSH] {
	color: #ffffff;
	text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
	background-color: #006dcc;
	background-image: gradient(linear, left top, left bottom, from(#428BCA),
		to(#428BCA));
	background-repeat: repeat-x;
	border: 1px solid #006dcc;
	border-radius: 4px;
	font: 14px Verdana, "Lucida Sans", Arial, Helvetica, sans-serif;
}

Button[PUSH]:pressed {
	color: #ffffff;
	background-color: #0044cc;
	background-image: gradient(linear, left top, left bottom, from(#226DA5),
		to(#226DA5));
	background-repeat: repeat-x;
	text-shadow: 0px -px 0px rgba(255, 255, 255, 0.4);
	font: 14px Verdana, "Lucida Sans", Arial, Helvetica, sans-serif;
}

Button[PUSH]:default {
	color: #ffffff;
	text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
	background-color: #006dcc;
	background-image: gradient(linear, left top, left bottom, from(#428BCA),
		to(#428BCA));
	background-repeat: repeat-x;
	border: 1px solid #006dcc;
	border-radius: 4px;
	font: 14px Verdana, "Lucida Sans", Arial, Helvetica, sans-serif;
}

Button[RADIO] {
	background-color: transparent;
}

Button.search {
	color: #fff;
	background-color: #0069aa;
	background-repeat: no-repeat;
	background-image: url( "/cu/uci/abos/platform/resources/search.png" );
	border: 1px solid #0069aa;
	background-position: center;
	border-radius: 4px !important;
}

Button.search:hover {
	box-shadow: 0 1px 0 #eefaeb inset;
	color: #073a93;
	background-color: #4094d0;
	background-repeat: no-repeat;
	background-image: url( "/cu/uci/abos/platform/resources/search.png" );
	background-position: center;
	margin: auto;
}

Button.hide_expander_button {
	background-image: url( "/cu/uci/abos/platform/resources/left.png" );
	background-repeat: no-repeat;
	border: 1px solid #9F9F9F;
	border-radius: 4px !important;
}

Button.hide_expander_button:hover {
	background-image: url( "/cu/uci/abos/platform/resources/left.png" );
	background-repeat: no-repeat;
	border: 1px solid #9F9F9F;
	border-radius: 4px !important;
}

Button.hide_expander_button:pressed {
	background-image: url( "/cu/uci/abos/platform/resources/left.png" );
	background-repeat: no-repeat;
	border: 1px solid #9F9F9F;
	border-radius: 4px !important;
}

Button[ARROW]:pressed,Button[PUSH]:pressed,Button[TOGGLE]:pressed {
	background-image: none;
	border: 1px solid #9F9F9F;
	border-radius: 4px !important;
}

/*Fin estilo botones*/

/*Estilo Textos*/
Text {
	border: 1px solid #9F9F9F;
	border-radius: 4px !important;
}

Text:focused {
	border: 1px solid #4094d0;
}

/*Fin estilo textos*/

/*Estilo de los elmento Links*/
Link {
	font: 14px Verdana, "Lucida Sans", Arial, Helvetica, sans-serif;
	color: #4a4a4a;
	background-color: transparent;
	background-image: none;
}

Link-Hyperlink {
	color: #4a4a4a;
	text-shadow: none;
	text-decoration: none;
	border-left-style: 1px solid #9F9F9F;
}

Link-Hyperlink:hover {
	text-decoration: underline;
}

Link-Hyperlink:selected {
	text-decoration: overline;
}

Combo.DropDown,Combo[BORDER] {
	font: 14px Verdana, "Lucida Sans", Arial, Helvetica, sans-serif;
	color: #4a4a4a;
	background-color: #ffffff;
	background-image: none;
	text-shadow: none;
	box-shadow: inset 0 0 3px #bdbdbd;
}

Composite.footerComposite {
	margin-top: 0px;
	background-color: #0162B1;
	background-image: none;
	background-repeat: repeat;
}

Composite.upload-panel {
	background-color: #dddddf;
	border-radius: 2px;
}

/*Este es el estilo del menu superior*/
Composite.menubar_background,Composite.content_background,Composite.main_menu_background
	{
	margin-top: 0px;
	background-color: #EBEBEB;
	background-image: none;
	background-repeat: repeat;
}

Composite.menubar_background {
	border: solid #0162B1 1px;
	box-shadow: 0 2px 3px #D2D2D2;
}

Composite.body_background {
	background-color: #EBEBEB;
	background-repeat: no-repeat;
}

Composite.header_background {
	background-color: #EBEBEB;
	background-repeat: no-repeat;
}

Composite.filter_menu_background {
	border: solid #0162B1 1px;
	background-color: #EBEBEB;
	background-repeat: no-repeat;
}
}

Composite.Login_Border {
	box-shadow: 0 0 3px 0 rgba(0, 0, 0, 0.2);
	border-radius: 0 0 4px 4px;
	border-width: 0 1px 1px;
	border-color: #A0A0A0;
}

Composite.workspace_content {
	padding: 10px 20px 10px 20px;
	background-color: #EBEBEB;
}

Composite.workspace_content_area {
	background-color: #F4F4F4;
}

Composite.Profile {
	background-color: #d6d6d6;
	margin: inherit;
}

Composite.expandItem_Margin_Border {
	padding: 10px 0px 0px 10px;
	background-color: #F4F4F4;
}

Composite.expandItem_Margin {
	padding: 10px 0px 0px 10px;
	background-color: #F4F4F4;
}

Composite.Login_Bottom_Background {
	background-color: #EBEBEB;
}

Composite.gray_background {
	background-color: #EBEBEB;
}

/*Estilo de group*/
Group {
	font: 14px Verdana, "Lucida Sans", Arial, Helvetica, sans-serif;
	color: #4a4a4a;
	background-color: #EBEBEB;
	border: none;
}

Shell.small_Login_Screen {
	padding: 50px 0px 0px 200px;
}

Shell.Principal {
	border: none;
	background-color: #EBEBEB;
}

ExpandItem {
	box-shadow: 0 4px 5px #D2D2D2;
}

Shell.Login_Background {
	background-color: #EBEBEB;
}

Shell-Titlebar {
	background-color: #0080c0;
	background-gradient-color: #0080c0;
	color: #666666;
	background-image: gradient(linear, left top, left bottom, from(#cad2d9),
		to(#d1d9de));
	padding: 2px 5px 2px;
	margin: 0px;
	height: 20px;
	border: none;
	border-radius: 3px 3px 0px 0px;
	text-shadow: none;
}

/*Estilo del ToolBar*/
ToolBar {
	background-color: transparent;
}

ToolItem {
	background-color: transparent;
}

/*Fin Estilo ToolBar*/
ExpandBar[BORDER] {
	border: none;
	border-radius: 0;
}

ExpandBar {
	color: #4a4a4a;
	background-color: #EBEBEB;
}

ExpandItem {
	border: 1px solid #bdbdbd;
	border-radius: 2px;
}

ExpandItem-Header {
	border: none;
	border-radius: 0;
	cursor: pointer;
	background-image: gradient(linear, left top, left bottom, from(#D6D6D6),
		to(#D6D6D6));
	text-shadow: none;
}